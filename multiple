// pages/cache1/cache1.js

// Laboratorio de Navegación (Cache1)
nav_switchTab() {
  const appInst = getApp();
  var from = (this.$page && this.$page.fullPath) || (this.route || 'pages/cache1/cache1');
  appInst.snapshotStack('switchTab', { to: '/pages/index/index', from: from });
  my.switchTab({ url: '/pages/index/index' });
},
nav_navigateTo() {
  const appInst = getApp();
  var from = (this.$page && this.$page.fullPath) || (this.route || 'pages/cache1/cache1');
  appInst.snapshotStack('navigateTo', { to: '/pages/cache1/cache1', from: from });
  my.navigateTo({ url: '/pages/cache1/cache1' });
},
nav_redirectTo() {
  const appInst = getApp();
  var from = (this.$page && this.$page.fullPath) || (this.route || 'pages/cache1/cache1');
  appInst.snapshotStack('redirectTo', { to: '/pages/cache2/cache2', from: from });
  my.redirectTo({ url: '/pages/cache2/cache2' });
},
nav_reLaunch() {
  const appInst = getApp();
  var from = (this.$page && this.$page.fullPath) || (this.route || 'pages/cache1/cache1');
  appInst.snapshotStack('reLaunch', { to: '/pages/index/index', from: from });
  my.reLaunch({ url: '/pages/index/index' });
