// pages/index/index.axml
<view>
  <!-- Información del Sistema -->
  <!-- Existing code here -->
  <view class="settings-section mt-16">
    <text class="subtitle">Laboratorio de Navegación (esta página)</text>
    <button class="btn btn-secondary" onTap="nav_switchTab_home">switchTab → Inicio</button>
    <button class="btn btn-primary" onTap="nav_navigateTo_cache1">navigateTo → Cache 1</button>
    <button class="btn btn-secondary" onTap="nav_redirectTo_cache2">redirectTo → Cache 2</button>
    <button class="btn btn-secondary" onTap="nav_reLaunch_home">reLaunch → Inicio</button>
    <button class="btn btn-secondary" onTap="nav_navigateBack">navigateBack ←</button>
  </view>
  <!-- Stack-panel or other existing code here -->
</view>

